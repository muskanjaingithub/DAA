#include<stdio.h>
#define max 100
void func(int[],int);
int main()
{
    int testcase;
    int a[max];
    int n;
    scanf("%d",&testcase);
    for(int i=0;i<=testcase;i++)
    {

    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
       scanf("%d",&a[i]);
    }
    func(a,n);
    }
}
void func(int a[],int n)
{
    int i,j,k,c=0;
    for(k=0;k<n;k++)
    {
        for(j=0;j<n;j++)
        {
            for(i=j+1;i<n;i++)
            {
                if(a[i]+a[j]==a[k])
                {
                c=1;
                printf("%d%d%d",i+1,j+1,k+1);
                }
            }
        }
    }
    if(c==0)
    printf("No sequence found\n");
}
